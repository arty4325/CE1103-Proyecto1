<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cemusicplayer.MusicPlayerController">
   <children>
      <Button layoutX="229.0" layoutY="207.0" mnemonicParsing="false" onAction="#play" text="Play" />
      <Button layoutX="331.0" layoutY="207.0" mnemonicParsing="false" onAction="#Pause" text="Pause" />
      <Button layoutX="331.0" layoutY="266.0" mnemonicParsing="false" onAction="#down" text="Menos" />
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#LSongs" prefHeight="25.0" prefWidth="96.0" text="Create Playlist" />
      <Button layoutX="14.0" layoutY="52.0" mnemonicParsing="false" onAction="#AddMusic" prefHeight="25.0" prefWidth="132.0" text="Add Music To Playlist" />
      <Button layoutX="229.0" layoutY="266.0" mnemonicParsing="false" onAction="#Up" text="mas" />
      <ComboBox fx:id="Playlist" layoutX="378.0" layoutY="27.0" onAction="#PlaylistChooser" prefWidth="150.0" promptText="Playlist" />
      <Button layoutX="453.0" layoutY="77.0" mnemonicParsing="false" onAction="#NextSong" text="Next" />
      <Button layoutX="378.0" layoutY="77.0" mnemonicParsing="false" onAction="#LastSong" text="Last" />
      <Button layoutX="485.0" layoutY="279.0" mnemonicParsing="false" onAction="#Loop" text="Loop" />
   </children>
</AnchorPane>
